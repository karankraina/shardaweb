</head>

<body>
    <div class="wrapper">
        <!-- Sidebar  -->
        <nav id="sidebar">
            <div id="dismiss">
                <i class="fas fa-arrow-left"></i>
            </div>

            <div class="sidebar-header">
                <h3>Core Sharda Team</h3>
            </div>
			<h4>Lal Vaakh</h4>
			<input class="form-control" id="myInput" type="text" placeholder="Enter Vaakh no">
            <ul class="list-unstyled components" id="myList">
                
							{{#each vaakharray}}
							<li>
                    <a href="/vaakh?number={{@index}}">{{this.title}}</a>
                </li>
							{{/each}}                
            </ul>

            <ul class="list-unstyled CTAs">
                <li>
                    <a href="https://play.google.com/store/apps/details?id=com.karan.raina.sharda.sharda" class="download">Download App</a>
                </li>
                
            </ul>
        </nav>
         

        <!-- Page Content  -->
        <div id="content">

            <nav class="navbar navbar-light bg-light fixed-top">
                

                    <button type="button" id="sidebarCollapse" class="btn btn-info">
                        <i class="fas fa-align-left"></i>
                        <span></span>
                    </button>
                    
